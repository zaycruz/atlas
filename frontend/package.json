{
  "name": "atlas-desktop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "backend:dev": "poetry run python frontend/python-backend/main.py",
    "dev:with-backend": "concurrently --kill-others-on-fail --kill-others --names vite,backend \"npm run dev -- --clearScreen=false\" \"poetry run python frontend/python-backend/main.py\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.8.4",
    "@types/node": "^24.6.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}
